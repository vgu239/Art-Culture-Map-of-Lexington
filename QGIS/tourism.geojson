{ "type": "Feature", "properties": { "full_id": "n2627329454", "osm_id": "2627329454", "artwork_type": "sculpture", "description": "Made in honor of the University of Kentucky’s first president James K. Patterson, this statue has stood on the campus’s grounds since 1934. The Scotland native oversaw the transformation of the three local colleges into the state university we see today.", "addr:postcode": null, "addr:housenumber": null, "addr:city": "Univeristy of Kentucky", "website": "https://exploreuk.uky.edu/catalog/xt75736m0s6q_280_3", "name": "James K. Patterson", "artist_name": "Augustus Lukeman" }, "geometry": { "type": "Point", "coordinates": [ -84.5044447, 38.0384997 ] } },
